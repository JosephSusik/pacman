<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.control.Label?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.pacman.controller.LoadMapController"
      style="-fx-background-color: black; -fx-font-family: Pixel Emulator;"
>
        <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
        </padding>

        <Button text="Select map" fx:id="btn_selectMap"/>
        <Label fx:id="label" style="-fx-text-fill: white"/>

        <Button text="Start game" fx:id="btn_startGame"/>
        <Label fx:id="start_label" style="-fx-text-fill: white"/>

        <Button text="Back" fx:id="btn_back"/>
</VBox>
